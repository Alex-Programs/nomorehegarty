{

  "manifest_version": 2,
  "name": "Hegarty Video Remover",
  "version": "1.1",

  "description": "Gets rid of the annoying Hegarty maths video popups.",

  "icons": {
    "48": "icons/48-final.png",
    "96": "icons/96-final.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.hegartymaths.com/*"],
      "css" : ["purge.css"]
    }
  ]

}
